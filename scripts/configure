#!/usr/bin/env bash

SCRIPTS_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" 
 # Get root directory of project
PROJECT_DIR=$(dirname $SCRIPTS_DIR)            

cd $PROJECT_DIR
# cmake -H. -Bbuild

echo
echo "-- ll bef"
ls -l 

echo
echo "-- 1) Download openssl"
wget https://www.openssl.org/source/old/1.0.2/openssl-1.0.2o.tar.gz

echo
echo "-- ll aft"
ls -l

echo
echo "-- df -h"
df -h


