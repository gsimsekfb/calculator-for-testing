#!/usr/bin/env bash

SCRIPTS_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" 
 # Get root directory of project
PROJECT_DIR=$(dirname $SCRIPTS_DIR)            

cd $PROJECT_DIR
# cmake -H. -Bbuild

echo "-- ll"
ls -l 

echo "-- 1) Download openssl"
wget ftp://ftp.openssl.org/source/old/1.0.2/openssl-1.0.2.tar.gz

echo "-- ll"
ls -l

echo "-- df -h"
df -h


